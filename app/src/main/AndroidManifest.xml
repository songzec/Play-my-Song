<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chens.PlaymySong">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.chens.PlaymySong.ui.cover.CoverActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                    <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.chens.PlaymySong.ui.main_page.MainActivity" />
        <activity android:name="com.example.chens.PlaymySong.ui.cover.LoginActivity" />
        <activity android:name="com.example.chens.PlaymySong.ui.cover.SignUpActivity" />
        <activity android:name="com.example.chens.PlaymySong.ui.settings.SettingActivity" />
        <activity android:name="com.example.chens.PlaymySong.ui.main_page.playing_page.PlayingActivity" />
        <activity android:name="com.example.chens.PlaymySong.ui.settings.AccountProfileActivity"
            android:label="@string/app_name"/>

        <activity android:name="com.example.chens.PlaymySong.ui.settings.WishListActivity"
            android:label="@string/app_name"
            />
        <activity android:name="com.example.chens.PlaymySong.ui.settings.HelpActivity"
            android:label="@string/app_name"/>
        <activity android:name="com.example.chens.PlaymySong.ui.settings.FeedbackActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name="com.example.chens.PlaymySong.ui.settings.RecentlyPlayedActivity"
            android:label="@string/app_name"
            />
        <activity android:name="com.example.chens.PlaymySong.ui.settings.MyFavoriteListActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.example.chens.PlaymySong.ui.settings.SettingActivity"
            >
        </activity>
    </application>

</manifest>
